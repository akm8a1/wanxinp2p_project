
.button{
	color:#ffffff;
	text-align: center;
	display: inline-block;
}
.but-med-blue {
	background: -o-linear-gradient(320deg, #8a8fef, #4f57eb);
	background: linear-gradient(130deg, #8a8fef, #4f57eb);
	padding: 10rpx 40rpx;
	border-radius: 100rpx;
}
.but-med-orange {
	background: #4f57eb;
	background: -o-linear-gradient(320deg, #ffbe03, #ff9b00);
	background: linear-gradient(130deg, #ffbe03, #ff9b00);
	padding: 10rpx 40rpx;
	border-radius: 100rpx;
}
.but-med-blue-empty {
	color: #4f57eb;
	padding: 6rpx 30rpx;
	border-radius: 100rpx;	
	border:solid 1px #4f57eb;
}
.but-med-gray-empty {
	color: #C8C7CC;
	padding: 6rpx 30rpx;
	border-radius: 100rpx;
	border:solid 1px #C8C7CC;
}
.but-big-orange {
	background: -o-linear-gradient(320deg, #ffbe03, #ff9b00);
	background: linear-gradient(130deg, #ffbe03, #ff9b00);
	padding: 10rpx 40rpx;
	border-radius: 100rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.but-big-blue {
	background: -o-linear-gradient(320deg, #8a8fef, #4f57eb);
	background: linear-gradient(130deg, #8a8fef, #4f57eb);
	padding: 10rpx 0rpx;
	line-height: 2;
	border-radius: 100rpx;
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.but-big-gray {
	background: #dbdee7;
	padding: 10rpx 0rpx;
	line-height: 2;
	border-radius: 100rpx;
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}

